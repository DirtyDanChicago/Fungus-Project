  m_GameObject: {fileID: 597292612}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: ec422cd568a9c4a31ad7c36d0572b9da, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  itemId: 6
  indentLevel: 0
  storyText: "He didn\u2019t realize the consequences using such a weapon. The greed
    of one man consumed the world as they knew it."
  description: 
  character: {fileID: 0}
  portrait: {fileID: 0}
  voiceOverClip: {fileID: 0}
