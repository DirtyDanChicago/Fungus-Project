  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 597292612}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: ec422cd568a9c4a31ad7c36d0572b9da, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  itemId: 4
  indentLevel: 0
  storyText: 'If he refused, the king would slaughter the kingdom of the elves. Not
    knowing his intentions with the weapon, but full of fear for his people, he gave
    it to him. '
  description: 
  character: {fileID: 0}
  portrait: {fileID: 0}
  voiceOverClip: {fileID: 0}
