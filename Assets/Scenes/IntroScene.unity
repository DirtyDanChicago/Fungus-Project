  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 597292612}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: ec422cd568a9c4a31ad7c36d0572b9da, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  itemId: 5
  indentLevel: 0
  storyText: The king had no intentions of sparing him, or anyone else, and he wanted
    to destroy the world around his kingdom. He wanted to be the last king standing.
  description: 
  character: {fileID: 0}
  portrait: {fileID: 0}
  voiceOverClip: {fileID: 0}
  showAlways: 1
  showCount: 1
  extendPrevious: 0
